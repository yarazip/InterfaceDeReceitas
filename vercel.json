{
    "rewrites": [
      {
        "source": "/",
        "destination": "/TodasReceitas/Receitas.html"
      },
      {
        "source": "/home",
        "destination": "/TodasReceitas/MainView/mainview.html"
      },
      {
        "source": "/sobre",
        "destination": "/MainView/mainview.html"
      },
      {
        "source": "/contato",
        "destination": "/MainView/mainview.html"
      },
      {
        "source": "/login",
        "destination": "/loginSenha/login.html"
      },
      {
        "source": "/esqueci",
        "destination": "/loginSenha/esquecisenha.html"
      },
      {
        "source": "/styles/(.*)",
        "destination": "/TodasReceitas/styles/$1"
      },
      {
        "source": "/js/(.*)",
        "destination": "/TodasReceitas/js/$1"
      }
    ]
  }
  